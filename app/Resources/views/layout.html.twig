	<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <link rel="stylesheet" type="text/css" href="{% block style %}{% endblock %}"/>
    <meta charset="utf-8"/>
	<style>
		#map{
			height: 500px;
            width: 60%;
            margin: 0 auto;
		}
	</style>
</head>

<body {% block onload %}{% endblock %}>

