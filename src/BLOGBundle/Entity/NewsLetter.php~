<?php

namespace BLOGBundle\Entity;

/**
 * NewsLetter
 */
class NewsLetter
{
    /**
     * @var int
     */
    private $id;



    /**
     * @var string
     */
    private $lien;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }


    /**
     * Set lien
     *
     * @param string $lien
     *
     * @return NewsLetter
     */
    public function setLien($lien)
    {
        $this->lien = $lien;

        return $this;
    }

    /**
     * Get lien
     *
     * @return string
     */
    public function getLien()
    {
        return $this->lien;
    }
}
